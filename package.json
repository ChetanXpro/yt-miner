{
  "name": "yt-miner",
  "version": "1.0.4",
  "description": "A powerful tool to mine video data from YouTube search results",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "publish": "npm run build; npm publish"
  },
  "keywords": [
    "youtube",
    "yt",
    "miner",
    "search",
    "scraper",
    "video",
    "data",
    "extractor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chetanxpro/yt-miner"
  },
  "homepage": "https://github.com/ChetanXpro/yt-miner#readme",
  "bugs": {
    "url": "https://github.com/ChetanXpro/yt-miner/issues"
  },
  "author": "chetan baliyan",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/puppeteer": "^5.4.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "puppeteer": "^20.0.0"

  }
}
